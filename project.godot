; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Dmre"
run/main_scene="res://scenes/main_menu.tscn"
config/icon="res://icon.png"

[autoload]

Params="*res://scenes/gui/params.tscn"

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[locale]

translations=PoolStringArray( "res://locale/locales.en.translation", "res://locale/locales.ru.translation", "res://locale/locales.uk.translation" )
translation_remaps={
"res://sprites/logo.png": PoolStringArray( "res://sprites/logo_en.png:en", "res://sprites/logo_uk.png:uk" ),
"res://sprites/logo2.png": PoolStringArray( "res://sprites/logo_uk2.png:uk", "res://sprites/logo_en2.png:en" ),
"res://sprites/logo3.png": PoolStringArray( "res://sprites/logo_en3.png:en", "res://sprites/logo_uk3.png:uk" )
}

[rendering]

environment/default_environment="res://default_env.tres"
